{
  "extends": [ "plugin:prettier/recommended"],
  "rules":{
    "prettier/prettier": ["error", { "singleQuote": true }]
  },
  "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 8,
        "ecmaFeatures": {
          "experimentalObjectRestSpread": true
        }
    },
    "env": {
        "es6": true
    }
}
