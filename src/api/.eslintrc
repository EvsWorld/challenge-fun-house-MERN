{
  "extends": [ "plugin:prettier/recommended"],
  "rules":{
    "prettier/prettier": ["error", { "singleQuote": true }]
  },
  "parcer": "babel-eslint",
  "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
          "experimentalObjectRestSpread": true
        }
    },

    "env": {
        "es6": true
    }
}
